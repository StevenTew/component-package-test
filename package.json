{
  "name": "@steventew/component-package-test",
  "version": "2.1.1",
  "description": "A simple test trialing npm for a component library",
  "author": "Steven Tew",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steventew/component-package-test.git"
  },
  "bugs": {
    "url": "https://github.com/steventew/component-package-test/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@storybook/addon-storyshots": "^6.2.9",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "react-test-renderer": "^17.0.2",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "clean-dist": "rimraf ./dist && mkdirp dist/css dist/assets",
    "predist": "npm-run-all clean-dist",
    "dist": "babel src/components --out-dir dist && cp src/index.css dist/css && cp -r src/assets dist/assets"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/node-logger": "^6.2.8",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.8",
    "@storybook/testing-react": "^0.0.10",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "homepage": "https://github.com/steventew/component-package-demo#readme",
  "main": "dist/index.js",
  "keywords": [
    "css",
    "sass",
    "react",
    "mobile-first",
    "component-library",
    "web",
    "magpie",
    "storybook"
  ],
  "files": [
    "dist"
  ]
}
